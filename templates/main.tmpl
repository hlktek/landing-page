                        </a>
                    </div>
                </div>
                
            </div>
            
        </div>
    </section>
    <!-- Section - Gallery Start -->
    <section id="all-games" class="section-all-games">
        <div class="container">
            
            <div class="row">
                <div class="col-12">
                    <div class="game-filter">
                        <ul class="mb-5 p-0 text-center">
                            <li class="list-inline-item"><a href="#" data-filter="*" class="btn btn-sm btn-filter-game btn-style-1 ">All</a></li>
                            <li class="list-inline-item"><a href="#" data-filter=".fish" class="btn btn-sm btn-filter-game btn-style-1">Fish game</a></li>
                            <li class="list-inline-item"><a href="#" data-filter=".slot" class="btn btn-sm btn-filter-game btn-style-1">Slot game</a></li>
                            <li class="list-inline-item"><a href="#" data-filter=".rng" class="btn btn-sm btn-filter-game btn-style-1">RNG game</a></li>
                            <li class="list-inline-item"><a href="#" data-filter=".rng" class="btn btn-sm btn-filter-game btn-style-1">Card game</a></li>
                        </ul>
                    </div>
                    <!-- //.game-filter -->
                    <div class="game-grid-wrapper">
                        <div class="game-grid grid-col-3 gutter-medium">
                    {{range .listGameInfo}}

                            <div class="item {{.Category}}">
                            <a {{if not .Token}}href="/auth/google"{{end}} {{if .Token}}href="{{.Link}}{{.Token}}"{{end}}>
                                <figure>
                                    <div class="game-img">
                                        <img src="/assets/images/games/game3.webp" alt="{{.Name}}">
                                    </div>
                                    <figcaption class="text-center">
                                        <div class="d-table color-white">
                                            <div class="d-table-cell align-middle">
                                                <h3 class="game-name">{{.Name}}</h3>
                                                <div class="game-amount">$<span>1.054.768.000</span></div>
                                            </div>
                                        </div>
                                    </figcaption>
                                </figure>
                                </a>
                            </div>
                            {{end}}
                        </div>
                        <!-- //.game-grid -->
                    </div>
                    <!-- //.game-grid-wrapper -->
                </div>
                <!-- //.col-12 -->
            </div>
            <!-- //.row -->
        </div>
        <!-- //.container -->
    </section>
