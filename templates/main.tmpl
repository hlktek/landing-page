                                    <a class="nav-link">Card</a>
                                </li>
                            </ul>
                            <ul id="top-winners-body" class="list-group list-group-flush list-jp">
                                {{range .topWinnerData}}
                                <li class="list-group-item">
                                    <div class="media">
                                        <div class="media-body">
                                            <div class="jp-winner-name">{{.DisplayName}}</div>
                                        </div>
                                        <div class="jp-winner-total">
