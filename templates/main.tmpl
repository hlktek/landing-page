    <section id="all-games" class="section-all-games">
        <div class="container-fluid">
            <div class="row">
                
                <div class="col-lg-9 col-md-7 col-12">
                    <div class="row">
                        <div class="col-lg-9 col-md-12 col-12">
                            <div class="game-filter pt-4 pb-1">
                                <ul class="mb-1 p-0">
                                    <li class="list-inline-item"><a href="#" data-filter="*" class="btn btn-sm btn-filter-game actived">All</a></li>
                                    <li class="list-inline-item"><a href="#" data-filter=".fish" class="btn btn-sm btn-filter-game ">Fish game</a></li>
                                    <li class="list-inline-item"><a href="#" data-filter=".slot" class="btn btn-sm btn-filter-game ">Slot game</a></li>
                                    <li class="list-inline-item"><a href="#" data-filter=".rng" class="btn btn-sm btn-filter-game ">RNG game</a></li>
                                    <li class="list-inline-item"><a href="#" data-filter=".card" class="btn btn-sm btn-filter-game ">Card game</a></li>
                                    <li class="list-inline-item"><a href="#" data-filter=".chess" class="btn btn-sm btn-filter-game ">Chess game</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-12 col-12">
                            <div class="game-search pt-4 pb-1">
                                <div class="form-group">
                                    <input type="text" placeholder="Search by Id or Name ..." autocomplete="off" class="form-control-plaintext color-white form-control form-control-sm" id="searchTerm">
                                </div>
                            </div>
                        </div>
                    </div>
                    
                    <!-- //.game-filter -->
                    <div class="game-grid-wrapper">
                        <div id="list-game" class="game-grid grid-col-4 gutter-medium row">
                            {{range .listGameInfo}}
                            {{if .Active}}
                                <div class="item col-xl-3 col-md-4 col-6 {{.Category}}">
                                    <a target="_blank" {{if not .Token}}href="/auth/google"{{end}} {{if .Token}}href="{{.Link}}{{.Token}}"{{end}}>
                                        <figure>
                                            <div class="game-img">
                                                <img src="/images?imageName=icon-{{.ServiceID}}.png" alt="{{.ServiceName}}" />
                                            </div>
                                            <figcaption class="text-center">
                                                <div class="d-table color-white">
                                                    <div class="d-table-cell align-middle">
                                                        <h3 class="game-name">{{.ServiceName}}</h3>
                                                        <div class="game-id invisible">{{.ServiceID}}</div>
                                                        <div class="mt-3">
                                                            <button class="btn btn-sm btn-filter-game btn-style-1">Chơi ngay</button>
                                                        </div>
                                                        {{/* <div class="game-amount">$<span>1.054.768.000</span></div> */}}
                                                    </div>
                                                </div>
                                            </figcaption>
                                        </figure>
                                    </a>
                                </div>
                                {{end}}
                            {{end}}
                        </div>
                    </div>
                </div>
                
                <div class="col-lg-3 col-md-5 col-12 bg-black">
                    {{if .topWinnerChessData}} 
                        <div class="aside-module pt-4 pb-4">
                            <div class="aside-module-header">
                                <h3>Top cao thủ cờ úp</h3>
                            </div>
                            <div class="aside-module-body">
                            <table class="table table-dark">
                                <thead>
                                    <tr>
                                    <th scope="col">User</th>
                                    <th class="text-center text-success">Thắng</th>
                                    <th class="text-center text-warning">Hoà</th>
                                    <th class="text-center text-danger">Thua</th>
                                    </tr>
                                </thead>
                                <tbody style="font-size: .85rem">
                                    {{range .topWinnerChessData}}
                                    <tr>
                                        <td>{{.User.DisplayName}}</td>
                                        <td class="text-center text-success">{{.TotalWinCount}}</td>
                                        <td class="text-center text-warning">{{.TotalDrawCount}}</td>
                                        <td class="text-center text-danger">{{.TotalLoseCount}}</td>
                                    </tr>
                                    {{end}}
                                </tbody>
                                </table>
