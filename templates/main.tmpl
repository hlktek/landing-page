    </section>
    
    <section class="section-games">
        <div class="container">
            <div class="section-header">
                <h2 class="text-center">Most Popular</h2>
            </div>
            <div class="row">
                <div class="col-lg-6 col-md-6 col-12">
                    <!-- Card -->
                    <div class="card  mb-4 card-hover">
                        <a href="/auth/google">
                            <img src="/assets/images/games/game1.webp" alt="game1" class=" card-img-top">
                        </a>
                    </div>
                    <div class="card  mb-4 card-hover">
                        <a href="/auth/google">
