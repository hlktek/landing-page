
<!doctype html>
<html>
<head>
    <title>Feedback - GemiTek</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/assets/libs/bootstrap-4.6.0/css/bootstrap.min.css">
     <link rel="stylesheet" href="/assets/libs/toastr/toastr.min.css">
     <link rel="stylesheet" href="/assets/icons/style.css">
    <link rel="stylesheet" href="/assets/styles/main.css?v=8">
</head>
<body>
<div>
    <nav class="navbar navbar-expand-lg navbar-dark">
        <div class="container-fluid">
            <a class="navbar-brand" href="/">GEMITEK</a>
            
             {{if not .token}}<a href="/auth/google" class="btn  btn-style-1 ml-auto">Login</a>{{end}}
             {{if .token}}
                
                <button class="btn btn-collaped-white d-md-none p-0 ml-3" type="button" data-toggle="collapse" data-target="#navbarMain" aria-controls="navbarMain" aria-expanded="false" aria-label="Toggle nav">
                    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 30 30" role="img" focusable="false"><title>Menu</title><path stroke="currentColor" stroke-linecap="round" stroke-miterlimit="10" stroke-width="2" d="M4 7h22M4 15h22M4 23h22"/></svg>
                </button>

                <div class="collapse navbar-collapse" id="navbarMain">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a class="nav-item nav-link" href="/feedback">
                                <span class=" icon-commenting-o"></span> Feedback
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-item nav-link" href="#" data-toggle="modal" data-target="#modalNaptien">
                                <span class="icon-dollar"></span> Nạp tiền
                            </a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                Hi, {{.displayName}}
                            </a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <button class="btn btn-link dropdown-item" id="logout">Logout</button>
                            </div>
                        </li>
                    </ul>
                </div>
            {{end}}
        </div>
    </nav>
    
    <section class="section-feedback bg-white" style="min-height: 89vh">
        <div class="container color-dark pt-5">
            <h1>Feedback</h1>
            <form id="frm-feedback" class="mt-4 ">
                <div class="form-group">
                    <label for="exampleInputEmail1">Email</label>
                    <input type="text" id="email" value="{{.email}}" class="form-control" readonly>
                </div>
                <div class="form-group">
                    <label for="serviceId">Game</label>
                    <select class="form-control" id="serviceId">
                        <option value="7993">Cờ up</option>
                    </select>
                </div>
                <div class="form-group">
                    <label for="content">Nội dung</label>
                    <textarea class="form-control" id="feedBack" rows="5"></textarea>
                </div>
                <button type="button" id="btn-show-confirm" class="btn btn-primary" style="min-width: 100px">Gửi</button>
            </form>
        </div>
    </section>
</div>

<footer class="footer">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-md-12 col-12 text-center pt-2 pb-2">
                <span>© 2021 Gemitek. All Rights Reserved.</span>
            </div>
        </div>
    </div>
</footer>

<!-- Modal feedback-->
<div class="modal fade" id="modalFeedback" tabindex="-1" aria-labelledby="naptienModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-sm modal-dialog-centered color-dark">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="naptienModalLabel">Feedback</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p>Bạn có muốn gửi feedback này?</p>
        <div class="mt-3 text-center">
            <button type="button" class="btn btn-primary btn-block" id="btn-send-feedback">Đồng ý</button>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- Modal Nap tien-->
<div class="modal fade" id="modalNaptien" tabindex="-1" aria-labelledby="naptienModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-sm modal-dialog-centered color-dark">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="naptienModalLabel">Nạp tiền</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p>Nạp 2.000.000 đ vào tài khoản?</p>
        <div class="mt-3 text-center">
            <button type="button" class="btn btn-primary btn-block" id="btn-naptien">Đồng ý</button>
        </div>
      </div>
    </div>
  </div>
</div>

<script src="/assets/libs/jquery/jquery-3.5.1.min.js"></script>
<script src="/assets/libs/toastr/toastr.min.js"></script>
<script src="/assets/libs/bootstrap-4.6.0/js/bootstrap.bundle.min.js"></script>
<script src="/assets/libs/isotope/isotope.pkgd.min.js"></script>
<script src="/assets/libs/imagesloaded/imagesloaded.pkgd.min.js"></script>
<script src="/assets/scripts/main.js?v=9"></script>

</body>
</html> 